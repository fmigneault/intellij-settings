<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="DejaVu Sans Mono for Powerline" />
    <option name="SECONDARY_FONT_FAMILY" value="Cousine for Powerline" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bnotes*\b.*">
      <option name="FOREGROUND" value="977ab" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>